{
  "name": "uscustomers",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "homepage": "https://www.customers.unoshops.com",
  "scripts": {
    "dev": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "build": "expo export --platform web --output-dir dist",
    "add-favicon": "node ./scripts/add-favicon.js",
    "predeploy": "npm run build && npm run fix-paths && npm run add-cname && npm run add-favicon",
    "fix-paths": "sed -i '' 's|=\"/_expo/|=\"./_expo/|g' dist/index.html",
    "add-cname": "echo 'customers.unoshops.com' > dist/CNAME && touch dist/.nojekyll",
    "deploy": "gh-pages -d dist --dotfiles"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@sekizlipenguen/react-native-popup-confirm-toast": "^1.0.7",
    "@shopify/flash-list": "^2.0.3",
    "@vis.gl/react-google-maps": "^1.5.5",
    "burnt": "^0.13.0",
    "crypto-js": "^4.2.0",
    "expo": "~54.0.6",
    "expo-constants": "~18.0.8",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~8.0.8",
    "expo-location": "^19.0.7",
    "expo-navigation-bar": "^5.0.8",
    "expo-router": "~6.0.3",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "firebase": "^12.2.1",
    "html2canvas": "^1.4.1",
    "nativewind": "^4.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-bouncy-checkbox": "^4.1.2",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-image-crop-picker": "^0.51.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-shadow-2": "^7.1.2",
    "react-native-view-shot": "^4.0.3",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "tailwindcss": "^3.4.17",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "gh-pages": "^6.3.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
